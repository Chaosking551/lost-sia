    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-draggable": "^4.4.6",
    "sass": "^1.77.1",
    "semantic-ui-css": "2.5.0",
    "semantic-ui-react": "^2.0.3"
  },
  "peerDependencies": {
    "lodash": "^4.17.15",
